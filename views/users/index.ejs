<div class="row" ng-controller="UsersCtrl" ng-init="init(<%= JSON.stringify(user_list) %>)">
	<div class="col-lg-12 col-md-12">
		<table>
			<thead>
				<th><pad content="{{ 'User Id' }}"/></th>
				<th><pad content="{{ 'User Name' }}"/></th>
				<th><pad content="{{ 'Email' }}"/></th>
				<th></th>
				<th><pad content="{{ 'Misc. Key' }}"></pad></th>
				<th><pad content="{{ 'Misc. Value' }}"></pad></th>
			</thead>

			<tbody>
				<tr ng-repeat="user in users">
					<td><pad model-type="users" key="uid" model="user"/></td>
					<td><pad model-type="users" key="username" model="user"/></td>
					<td><pad model-type="users" key="email" model="user"/></td>
					<td></td>
					<td><pad content="{{user.miscKey}}"/></td>
					<td><pad content="{{user.miscVal}}"/></td>
				</tr>
	
			</tbody>
		</table>

		<div class="space2"></div>
		<a href="/users/new">
			<span class="label label-primary">Create A New User</span>
		</a>
		<div class="space2"></div>
		<a href="/">Back to Home Page</a>
	</div>
</div>
